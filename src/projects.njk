---
layout: base.njk
title: Projects
---

<div
  class="
    container
    my-5
  "
>

{# <h1 class="display-4 mb-2">Featured Projects</h1> #}
  <div class="row">
    {% for article in featuredProjects %}
    <div class="col-sm-6 col-md-4 my-3 d-flex justify-content-center align-items-center">
      <div class="card" style="width: 25rem">
        <img
          class="card-img-top"
          src="{{ article.imgURL }}"
          alt="{{ article.imgAlt }}"
        />
        <div class="card-body">
          <h5 class="card-title">{{ article.title }}</h5>
          <p class="card-text">{{ article.excerpt }}</p>
          <a href="/{{ article.slug }}" class="btn btn-primary">Read More</a>
        </div>
      </div>
    </div>
    {% endfor %}
  </div>

    {# <h1 class="display-4">All Projects</h1> #}
  <div class="row">
    {% for article in projects %}
    <div class="col-sm-6 col-md-4 my-3 d-flex justify-content-center align-items-center">
      <div class="card" style="width: 25rem">
        <img
          class="card-img-top"
          src="{{ article.imgURL }}"
          alt="{{ article.imgAlt }}"
        />
        <div class="card-body">
          <h5 class="card-title">{{ article.title }}</h5>
          <p class="card-text">{{ article.excerpt }}</p>
          <a href="/{{ article.slug }}" class="btn btn-primary">Read More</a>
        </div>
      </div>
    </div>
    {% endfor %}
  </div>
</div>
</div>


